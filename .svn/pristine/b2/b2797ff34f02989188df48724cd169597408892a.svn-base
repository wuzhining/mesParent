</script>    
<script type="text/javascript">
layui.config({
    base: '/static/plugins/layui/base/',
    v: new Date().getTime()
}).use([ 'form','table','laydate','layuiconfig','layuiformajax','layuitable'], function(){
	var form = layui.form, 
	$ = layui.jquery, table = layui.table,  laydate = layui.laydate,
	layer = layui.layer,  layuitable = layui.layuitable,layuiconfig = layui.layuiconfig;
	var layuiformajax = layui.layuiformajax;
	
	var curTable = layuitable.render({
    	elem: '#listTable'
    	,id:'listTable'
    	,height: 350
    	,url:'/bill/billWarehouseWorkOrderReturn/list/json?billTypeDictId='+10264
    	,limits:[5,10,15,20]
    	,cols: [[
    	     {field:'billCode', width:180, title: '单据编码',align:'center'}
    	    ,{field:'billTypeDic',  title: '单据类型',align:'center', minWidth: 100,templet:'#billTypeDic'}
    	    ,{field:'billStatusDic',  title: '单据状态',align:'center', minWidth: 80,templet:'#billStatusDic'}
    	    ,{field:'fromBill',  title: '来源单编码', align:'center',minWidth: 180,templet:'#fromBill'}
    	    ,{field:'fromBillTypeDic',  title: '来源单据类型',align:'center', minWidth: 100,templet:'#fromBillTypeDic'}   
    	    ,{field:'createTime',  title: '创建时间', align:'center',minWidth: 120}
    	    ,{field:'', width:'10%', title: '操作', align:'center', toolbar: '#barDemo', fixed: 'right'}
    	]]
  	});
	
	var renderTable = function (billId) {
		layuitable.render({
	    	elem: '#listTable_details'
	    	,id:'listTable_details'
	    	,url:'/bill/billWarehouseItem/list/json?billId='+billId
	    	,limits:[5,10,15,20]
	    	,cols: [[
				 {field:'productType', width:120, title: '产品分类',align:'center',templet:'#productType'}
				,{field:'productCode', width:120, title: '产品编码',align:'center',templet:'#productCode'}
				,{field:'product', width:120, title: '产品名称',align:'center',templet:'#product'}
	    	    ,{field:'MaterialCode', width:120, title: '产品规格编码',align:'center',templet:'#MaterialCode'}
	    	    ,{field:'materialName', width:120, title: '产品规格名称',align:'center',templet:'#productMaterial'}
	    	    ,{field:'sku', title: '产品规格值', align:'center',minWidth: 220,templet: '#sku'}
	    	    ,{field:'billStatus',  title: '单据状态',align:'center', minWidth: 120,templet:'#billStatus'}
	    	    ,{field:'quantity',  title: '单据数量',align:'center', minWidth: 120}
	    	    ,{field:'quantityFinish',  title: '完成数量',align:'center', minWidth: 120}
	    	]]
	  	});
	}
	
	//搜索
	form.on('submit(search)', function (data) {
		var billCode=data.field.billCode!=null?data.field.billCode:"";
		table.reload('listTable',{
			url:'/bill/billWarehouseWorkOrderReturn/list/json?billTypeDictId='+10264
			,where:{
				"billCode":billCode,
				"fromBillTypeDictId":data.field.fromBillTypeDictId,
				"billStatusDictId":data.field.billStatusDictId
			}
		});
    	return false;
		});
	
	table.on('tool(tablefilter)', function(obj) {
		var data = obj.data;
		if (obj.event === 'edit') { 
			layer_window_show('编辑供应商发货单','/bill/billWarehouseDelivery/edit?id=' + data.billCode); 
		}if(obj.event === 'lookOver'){
			layer_window_show('查看退料单','/bill/billWarehouseWorkOrderReturn/lookOver?id=' + data.id); 
		}
	});
	
	//初始化
	renderTable(0);
    //监听行单击事件（单击事件为：rowDouble） 
    table.on('row(tablefilter)', function(obj){
    	var data = obj.data;
    	$("#bill").html(data.billCode); 
      	//根据id加载
    	renderTable(data.id);
      	//标注选中样式
      	obj.tr.addClass('layui-table-click').siblings().removeClass('layui-table-click');
    });
	
	
	
	laydate.render({
		elem: '#startTime' //指定元素
		,type: 'date'
		,format: 'yyyy-MM-dd' //可任意组合
	});
	
	laydate.render({
		elem: '#endTime' //指定元素
		,type: 'date'
		,format: 'yyyy-MM-dd' //可任意组合
	});
});	
</script>
 
