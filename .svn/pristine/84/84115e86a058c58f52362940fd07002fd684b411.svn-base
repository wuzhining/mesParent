<script type="text/javascript">
layui.config({
    base: '/static/plugins/layui/base/',
    v: new Date().getTime()
}).use([ 'form','table','laydate','layuiconfig','layuiformajax','layuitable'], function(){
	var form = layui.form, 
	$ = layui.jquery, table = layui.table,  laydate = layui.laydate,
	layer = layui.layer,  layuitable = layui.layuitable,layuiconfig = layui.layuiconfig;
	var layuiformajax = layui.layuiformajax;
	
	var curTable = layuitable.render({
    	elem: '#listTable'
    	,id:'listTable'
    	,url:'/firm/firmPartner/list/json'
    	,cols: [[
    	    {field:'id',  title: 'ID',align:'center'}
    	    ,{field:'partnerTypeDictId',  title: '合作伙伴类型id',align:'center'}
    	    ,{field:'partnerTypeDictName',width:120,  title: '合作伙伴类型',align:'center',templet:'#partnerTypeDictName'}
    	    /* ,{field:'companyCode',  title: '公司编码', align:'center'} */
    	    ,{field:'companyName', width:200, title: '公司名称', align:'center'}
    	    ,{field:'simpleName',  title: '简称', align:'center'}
    	    ,{field:'status',  title: '状态',align:'center',templet:'#isValidTpl'}
    	    ,{field:'userId', width:100, title: '用户',align:'center'}
    	    ,{field:'countryName',  title: '国家', align:'center',templet:'#countryName'}
    	    ,{field:'areaName', width:100, title: '省',align:'center',templet:'#areaNameP'}
    	    ,{field:'areaName',  title: '市',align:'center',templet:'#areaNameC'}
    	    ,{field:'areaName',  title: '区',align:'center',templet:'#areaName'}
    	    ,{field:'address',  title: '地址', align:'center'}
    	    ,{field:'contactPhone',  title: '联系人电话', align:'center'}
    	    ,{field:'contactName',  title: '联系人姓名', align:'center'}
    	    ,{field:'email',  title: '电子邮箱', align:'center',minWidth: 190}
    	    ,{field:'remark',  title: '备注', align:'center'}
      		,{field:'', width:'10%',title: '操作', align:'center', toolbar: '#barDemo', fixed: 'right'}
    	]]
		,done: function () {
	        $("[data-field='id']").css('display','none');
	        $("[data-field='partnerTypeDictId']").css('display','none');
	    }
  	});
	
	form.on('submit(search)', function (data) {
		layuitable.reload(data.field);
        return false;
  	});
 
	
	table.on('tool(tablefilter)', function(obj) {
		var data = obj.data;
		if (obj.event === 'edit') { 
			layer_window_show('编辑合作伙伴公司','/firm/firmPartner/edit?id=' + data.id); 
		}
	});
	$('#reset').on('click',function(e){
   		$("input").val("");
   		$("select").val("");
   	})
});	
</script>
 
