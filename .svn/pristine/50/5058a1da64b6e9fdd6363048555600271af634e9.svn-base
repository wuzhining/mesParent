<style type="text/css">
        .layui-table-cell {
            height: auto;
            line-height: 28px;
            vertical-align:middle;
        }
      
</style>
<script type="text/html" id="partnerTypeDictName">
	{{#  if(d.configDictionary){ }}
		{{ d.configDictionary.dictName 

 }}
{{#  } }}
