<!DOCTYPE html>
<html>
  
  <head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" /> 
	<#include "../common-header.html"/>
  </head>
  
  <body>
    <div class="x-body">
      <form id="dataForm"  method="post" class="layui-form">
        <input type="hidden"  id="id"  name="id" value="<#if entity??&&entity.id??>${entity.id?c}<#else></#if>" />
        <input type="hidden"  id="version"  name="version" value="<#if entity??&&entity.version??>${entity.version?c}<#else></#if>" />
        
        <table class="layui-table ">
          <tr>  
               <td width="15%" class="tableleft">工具编码</td>
               <td>
                  <#if entity??&&entity.id??>${(entity.fixtureCode)!''}
                  <#else>
                   <input type="text" name="fixtureCode" class="layui-input"   maxlength="32"  />
                  </#if> 
               	 
              </td>
           	 <td width="15%" class="tableleft">工具标签</td>
               <td>
               	<input type="text" name="fixtureLabel" class="layui-input" maxlength="30" value="${(entity.fixtureLabel)!''}"/>
               </td>
             </tr> 
             
           <tr> 
           	 <td width="10%" class="tableleft" >工具品种规格</td>
                 <td>
		               <input type="text" id="fixtureSpecsName" name="fixtureSpecsName" disabled="disabled"  class="layui-input input-choiceBox-intd" value="${(entity.equipSpecsFixture.fixtureSpecsName)!''}"/>
		               <input type="hidden" id="specsFixtureId" name="specsFixtureId" class="layui-input input-choiceBox-intd"  value="<#if entity??&&entity.specsFixtureId??>${entity.specsFixtureId?c}<#else> </#if>"/>
		                <button class="layui-btn  layui-btn-primary btn-choiceBox-intd" id="equipFixtureSearch">
				           <i class="layui-icon layui-icon-more"></i>
				       </button>
               </td>
                <td width="10%" class="tableleft">标准寿命</td>
               <td>
               	<input type="text" name="standarlive" class="layui-input" maxlength="20" value="<#if entity??&&entity.standarlive??>${entity.standarlive?c}<#else></#if>"/>
               </td>
           </tr>    
                
               
           <tr>                 
               <td width="15%" class="tableleft">工具状态</td>
              	<td>
               	<select name="fixtureStatusDictId"> 
                   <#if fixtureStatus??>
                    <#list fixtureStatus as item>
		            <option value="${item.id?c}" <#if (entity.fixtureStatusDictId??&&entity.fixtureStatusDictId==item.id)>selected="selected"</#if>>${item.dictName}</option>
		         </#list>
		         </#if>
		        </select>
             </td>
             <td width="15%" class="tableleft">供应商</td>
                <td>
                	<div class="div-choiceBox-intd ">  
                		<input type="hidden"  id="supplierId" name="supplierId" disabled="disabled" class="layui-input input-choiceBox-intd" value="<#if entity??&&entity.supplierId??>${entity.supplierId?c}<#else></#if>">
                		<input type="text" id="companyName" name="companyName"  disabled="disabled" class="layui-input input-choiceBox-intd" value="${(entity.firmPartner.companyName)!''}"/>
		          		<button class="layui-btn  layui-btn-primary btn-choiceBox-intd" id="supplierSearch"> 
		          			<i class="layui-icon layui-icon-more"></i>   
		          		</button>
	         	 	</div> 
               </td>
             </tr>  
             <tr>  
                <td width="10%" class="tableleft">仓库</td>
              	  <td>
               		<input type="hidden"  id="warehouseId" name="warehouseId" disabled="disabled" class="layui-input input-choiceBox-intd" value="<#if entity??&&entity.warehouseId??>${entity.warehouseId?c}<#else></#if>">
	              	<input type="text" id="warehouseName" name="warehouseName" disabled="disabled" class="layui-input input-choiceBox-intd" value="${(entity.structWarehouse.warehouseName)!''}">
	          		  <button class="layui-btn  layui-btn-primary btn-choiceBox-intd" id="warehouseSearch">
	          			<i class="layui-icon layui-icon-more"></i>
	          		   </button>
                 </td>  
               <td width="10%" class="tableleft">仓库货位</td>
                   <td>
                	<div class="div-choiceBox-intd ">   
                		<input type="hidden"  id="locationId" name="locationId" disabled="disabled" class="layui-input input-choiceBox-intd" value="<#if entity??&&entity.locationId??>${entity.locationId?c}<#else></#if>">
                		<input type="text" id="locationName" name="locationName" disabled="disabled" class="layui-input input-choiceBox-intd" value="${(entity.warehouseLocation.locationName)!''}"/>
		          		<button class="layui-btn  layui-btn-primary btn-choiceBox-intd" id="locationNameSearch"> 
		          			<i class="layui-icon layui-icon-more"></i>   
		          		</button>
	         	 	</div>
                </td>
             </tr>  
            
             <tr>   
                <td width="15%" class="tableleft">车间区域</td>
	              <td>
                	<div class="div-choiceBox-intd ">   
                		<input type="hidden"  id="workshopAreaId" name="workshopAreaId" disabled="disabled" class="layui-input input-choiceBox-intd" value="<#if entity??&&entity.workshopAreaId??>${entity.workshopAreaId?c}<#else></#if>">
                		<input type="text" id="areaName" name="areaName" value="${(entity.structWorkshopArea.areaName)!''}"  class="layui-input input-choiceBox-intd"/>
		          		<button class="layui-btn  layui-btn-primary btn-choiceBox-intd" id="workshopAreaSearch"> 
		          			<i class="layui-icon layui-icon-more"></i>   
		          		</button>
	         	 	</div>
                </td> 
               <td width="15%" class="tableleft">车间</td>
               <td>
                    <input tuype ="text"  id="workshopName" name="workshopName"  disabled="disabled" class="layui-input" value="${(entity.structWorkshop.workshopName)!''}"/>  
                    <input type="hidden"  id="workshopId" name="workshopId"   class="layui-input input-choiceBox-intd" value="<#if entity??&&entity.workshopId??>${entity.workshopId?c}<#else></#if>">
			   </td>
             </tr>  
              <tr>   
	             <td width="15%" class="tableleft">工厂名称</td>
                <td>
                     <input tuype ="text"  id="factoryName" name="factoryName"  disabled="disabled" class="layui-input" value="${(equipFactory1.factoryName)!''}" />  
                     <input type="hidden"  id="factoryId" name="factoryId"  disabled="disabled" class="layui-input input-choiceBox-intd" value="<#if entity??&&entity.factoryId??>${entity.factoryId?c}<#else></#if>">
                </td> 
                <td width="15%" class="tableleft">工位</td>
	              <td>
                	<div class="div-choiceBox-intd ">   
                		<input type="hidden"  id="workstationId" name="workstationId" disabled="disabled" class="layui-input input-choiceBox-intd" value="<#if entity??&&entity.workstationId??>${entity.workstationId?c}<#else></#if>">
                		<input type="text" id="workstationName" name="workstationName" value="${(structWorkstation.workstationName)!''}"  class="layui-input input-choiceBox-intd"/>
		          		<button class="layui-btn  layui-btn-primary btn-choiceBox-intd" id="firmPostSearch"> 
		          			<i class="layui-icon layui-icon-more"></i>   
		          		</button>
	         	 	</div>
                </td> 
               
             </tr> 
             <tr>  
               <td width="15%" class="tableleft">数量</td>
              	<td>
               	<input type="text" name="quantity" class="layui-input" maxlength="16" value="<#if entity??&&entity.quantity??>${entity.quantity?c}<#else>1</#if>"/>
             </td>
           	 <td width="15%" class="tableleft">已使用次数</td>
               <td>
               	<input type="text" name="numberUsed" class="layui-input" maxlength="20" value="<#if entity??&&entity.numberUsed??>${entity.numberUsed?c}<#else>0</#if>"/>
               </td>
             </tr> 
             <tr>  
           	 <td width="15%" class="tableleft">校验日期</td>
               <td>
               	<input class="layui-input" autocomplete="off" placeholder="校验日期" name="dateConfirm" id="dateConfirm" maxlength="20" value="${(entity.dateConfirm)!''}"/>
               </td>
                 <td width="15%" class="tableleft">到期日期</td>
              	<td>
              	<input class="layui-input" autocomplete="off" placeholder="到期日期" name="dateExpire" id="dateExpire" maxlength="20" value="${(entity.dateExpire)!''}"/>
               </td>
             </tr>  
           
             <tr> 
               <td width="15%" class="tableleft">校验确认时间</td>
              	<td>
               	<input class="layui-input" autocomplete="off" placeholder="校验确认时间" name="confirmTime" id="confirmTime" maxlength="20"/>
             </td>
             <td width="15%" class="tableleft">校验结果</td>
               <td>
               	<input type="text" name="confirmResult" class="layui-input" maxlength="20" value="${(entity.confirmResult)!''}"/>
             </td>
            </tr> 
              <tr>   
           	 <td width="15%" class="tableleft">校验确认人</td>
               <td>
               	<select name="confirmUserId"> 
                    <#if fEmployees??>
                     <#list fEmployees as item>
			            <option value="${item.id?c}" <#if (entity.confirmUserId??&&entity.confirmUserId==item.id)>selected="selected"</#if>>${item.realName}</option>
			         </#list>
			         </#if>
			    </select>
               </td>
                <td width="15%" class="tableleft">描述</td>
               <td colspan="3">
               	<input cops type="text" name="remark" class="layui-input" maxlength="20" value="${(entity.remark)!''}"/>
               </td> 
             </tr>
       
           
           <tr>
            <td colspan="4"  style="text-align:center">
               <button class="layui-btn" lay-submit="" lay-filter="save">保存</button>	
            </td>
           </tr>
       </table>  
      </form>
    </div>
    <script>
    var ids = $('#id').val();
    if(ids ==null || ids ==""){
  	  	$("#td1").hide();
  	  	$("#td2").hide();
    }
    layui.config({
        base: '/static/plugins/layui/base/',
        v: new Date().getTime()
    }).use([ 'form','laydate','layuiconfig','layuiformajax'], function(){
          $ = layui.jquery,  laydate = layui.laydate;
          var form = layui.form,layer = layui.layer;
          var layuiformajax = layui.layuiformajax,layuitable = layui.layuitable;
           var ids = $("#id").val();
          /* var url = $(window.parent.document).find("iframe").attr("src");
      	  var arr = url.split('?');
      	  var result=url.indexOf("?");
      	  var warehouseName=getParam("warehouseName"); */
      	  if(result>0){
      		/* var warehouseName = arr[1].split('&&')[1].split('=')[1];
      		$("#warehouseName").val(warehouseName); 
      		var locationName = arr[1].split('&&')[2].split('=')[1];
      		$("#locationName").val(locationName); 
      		var workshopName = arr[1].split('&&')[3].split('=')[1];
      		$("#workshopName").val(workshopName); 
      		var areaName = arr[1].split('&&')[4].split('=')[1];
      		$("#areaName").val(areaName);  */
      		/* if(warehouseName != null && warehouseName != ""){
      			$('#warehouseSearch').attr('disabled',true);
      		}
      		if(areaName != null && areaName != ""){
      			$('#workshopAreaSearch').attr('disabled',true);
      		} */
      	  } 
      	  
	      	$('#warehouseSearch').on('click',function(e){
	  	   		openWinSelect('/struct/structWarehouse/structWarehouseOpen',"warehouseId","warehouseName");
	  	   		e.preventDefault();//默认from表单提交时不刷新
	  	   	});
	  	   		$('#workstationSearch').on('click',function(e){
	  	   		openWinSelect('/struct/structWarehouse/structWarehouseOpen',"warehouseId","warehouseName");
	  	   		e.preventDefault();//默认from表单提交时不刷新
	  	   	});
	  	   		//工站
	  	  	$('#firmPostSearch').on('click',function(e){
	  	  	 openWinSelect('/struct/structWorkstation/structWorkstationOpen',"workstationId","workstationName");
	  	   		e.preventDefault();//默认from表单提交时不刷新
	  	   	});
	  	  $('#equipFixtureSearch').on('click',function(e){
        	  openWinSelect('/equip/equipSpecsFixture/listEquipFixture',"specsFixtureId","fixtureSpecsName");
  	   		  e.preventDefault();//默认from表单提交时不刷新
  	   	  });
	  	 
          $('#supplierSearch').on('click',function(e){
      		openWinSelect('/firm/firmPartner/firmPartnerOpen',"supplierId","companyName");
         		e.preventDefault();//默认from表单提交时不刷新
          }); 
          $('#locationNameSearch').on('click',function(e){
        		openWinSelect('/warehouse/warehouseLocation/warehouseLocationOpen',"locationId","locationName");
           		e.preventDefault();//默认from表单提交时不刷新
            }); 
	  	  $('#workshopAreaSearch').on('click',function(e){
	  		  var warehouseName=$("#warehouseName").val();
	  		 var w1='structWorkshop';
	  		  var w2='workshopId';
	  		  var w3='workshopName';
	  		/*   if(warehouseName!=''){
	  			 w1='';
	  			 w2='';
	  			 w3='';
	  		  }else{
	  			  $("#warehouseSearch").attr('disabled',true);
	  		  }
	  		  */
	  		openWinSelectCurrency(
    	    		'/struct/structWorkshopArea/structWorkshopAreaOpen'
    	    		,"workshopAreaId"
    	    		,"areaName"
    	    		,"structFactory"
    	    		,"factoryId"
    	    		,"factoryName"
    	    		,w1
    	    		,w2
    	    		,w3);	  
      	    
      	/*   ,"structWorkshop"
  		,"workshopId"
  		,"workshopName"); */
       		e.preventDefault();//默认from表单提交时不刷新
        })
	      
          //选择仓库查货位级联监听
          form.on('select(warehouseSelect)', function(data){
        	  $("#locationId option[value!='']").remove();
        	  linkWarehouseLocationByWarehouse(data.value,'locationId','locationName','','/product/productPackbox/linkLocationByWarehouse');
        	  $("#locationId").get(0).selectedIndex=0;
          });

          //监听提交
          form.on('submit(save)', function(data){
        	  console.log(data.field);
        	  var url = '/equip/equipFixture/save';
        	  layuiformajax.postJson(url, data.field, function (result) {
	      	    	 if (result.isSuccess) {
	         	    	   layer.alert("保存成功", {icon: 6},function () {
	       	                // 获得frame索引
	       	                var index = parent.layer.getFrameIndex(window.name);
	       				  	parent.location.reload(); 
	       	                parent.layer.close(index);
	       	            });	
	      			}else{
	      				layer.alert(result.message, {
	      					icon : 2
	      				});
	      			}
      		  });
      		  
            return false;
          });
          
            laydate.render({
	      		elem: '#dateConfirm' //指定元素
	      		,type: 'date'
	      		,format: 'yyyy-MM-dd' //可任意组合
	      	});
	      	
	      	laydate.render({
	      		elem: '#dateExpire' //指定元素
	      		,type: 'date'
	      		,format: 'yyyy-MM-dd' //可任意组合
	      	});
	      	laydate.render({
	      		elem: '#confirmTime' //指定元素
	      		,type: 'date'
	      		,format: 'yyyy-MM-dd' //可任意组合
	      	});
          
        });
    </script>
    <script>
	    $(function () {
	    	//初始化
	    	linkWarehouseLocationByWarehouse($("#warehouseId").val(),'locationId','locationName',$("#initWarehouseLocation").val(),'/product/productPackbox/linkLocationByWarehouse');
	    	
	    });
    </script>
  </body>

</html>