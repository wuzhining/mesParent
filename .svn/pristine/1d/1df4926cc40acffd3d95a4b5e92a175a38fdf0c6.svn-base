<!DOCTYPE html>
<html>
  
  <head> 
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" /> 
	<#include "../common-header.html"/>
  </head>
   <script type="text/html" id="isValidTpl">
    {{#  if(d.isValid == '1'){ }}
		是
	{{#  }else { }}
		否
	{{#  } }}
  </script> 
  <script type="text/html" id="factoryTypeName">
	{{#  if(d.configDictionary){}}
		{{ d.configDictionary.dictName 
 	}}
	{{# } }}
  </script>
  
  <body>
    <div class="x-nav">
      <span class="layui-breadcrumb"> 
        <a href="">工厂构造</a> <a><cite>工厂</cite></a>
      </span>
       <!--
       <a class="layui-btn layui-btn-small" style="line-height:1.6em;margin-top:3px;float:right" href="javascript:location.replace(location.href);" title="刷新">
       <i class="layui-icon" style="line-height:30px">ဂ</i></a>
       -->
    </div>
    <div class="x-body">
      <div class="layui-row">
        <form class="layui-form layui-col-md12 x-so">
          <input type="text" name="likeFactoryName"  placeholder="请输入工厂名称" autocomplete="off" class="layui-input">
           <input type="text" name="likeFactoryId"  placeholder="请输入工厂编码" autocomplete="off" class="layui-input">
           <select name="factoryTypeDictId">    
                <option value="">请选择工厂类型</option> 
                 <#if dictionaryList??>
                     <#list dictionaryList as item>
			            <option value="${item.id?c}">${item.dictName}</option>
			         </#list>
			         </#if>
          </select>
          <button class="layui-btn layui-btn-sm"  lay-submit lay-filter="search"><i class="layui-icon">&#xe615;</i></button>
          <button type="reset" class="layui-btn layui-btn-sm" id="reset" style="background-color: #009688;color:#FFFFFF">重置</button>
        </form>
      </div>
      <xblock>
        <!-- <button class="layui-btn" onclick="delAll()"><i class="layui-icon"></i>删除</button> -->
        <button class="layui-btn layui-btn-sm" onclick="layer_window_show('添加工厂','/struct/structFactory/edit')"><i class="layui-icon"></i>添加</button>
        <span class="x-right" style="line-height:40px"></span>
      </xblock>
       <div class="layui-col-md12">
  	 			<div class="layui-row grid-demo">
            		<div class="layui-col-md12" style="text-align: center;">
        					<table class="layui-hide" id="listTable" lay-filter="tablefilter"></table>
        			</div>
        		</div>
        </div>
     
     

    </div>
<script type="text/html" id="barDemo">	
	<a class="layui-btn layui-btn-xs  layui-btn-normal" lay-event="edit">编辑</a>
</script>    
<script type="text/javascript">
layui.config({
    base: '/static/plugins/layui/base/',
    v: new Date().getTime()
}).use([ 'form','table','laydate','layuiconfig','layuiformajax','layuitable'], function(){
	var form = layui.form, 
	$ = layui.jquery, table = layui.table,  laydate = layui.laydate,
	layer = layui.layer,  layuitable = layui.layuitable,layuiconfig = layui.layuiconfig;
	var layuiformajax = layui.layuiformajax;
	
	var curTable = layuitable.render({
    	elem: '#listTable'
    	,id:'listTable'
    	,url:'/struct/structFactory/list/json'
    	,cols: [[
    	    {field:'id', width:120, title: 'ID',align:'center'}
    	    ,{field:'factoryCode',  title: '工厂编码',align:'center'}
    	    ,{field:'factoryName',  title: '工厂名称',align:'center'}
    	    ,{field:'factoryTypeName',  title: '工厂类型', align:'center',templet:'#factoryTypeName'}
    	    ,{field:'isValid',  title: '是否有效', align:'center',templet:'#isValidTpl'}
    	    ,{field:'address',  title: '地址', align:'center',minWidth: 400}
    	    ,{field:'remark',  title: '备注', align:'center'}
    	    ,{field:'createTime',  title: '创建时间', align:'center',minWidth: 200}
      		,{field:'', title: '操作', align:'center', toolbar: '#barDemo', fixed: 'right'}
    	]],
        done: function () {
            $(".layui-table-box").find("[data-field='id']").css("display","none");   
         }
  	});
	
	form.on('submit(search)', function (data) {
		layuitable.reload(data.field);
        return false;
  	});
 
	
	table.on('tool(tablefilter)', function(obj) {
		var data = obj.data;
		if (obj.event === 'edit') { 
			layer_window_show('编辑工厂','/struct/structFactory/edit?id=' + data.id); 
		}
	});
	
});	
</script>
 
  </body>

</html>