	</select>
	<sql id="Date_Condition_sql">	
	</sql>

	<sql id="Foreign_Key_Condition_Sql">
	
	</sql>
	
	<sql id="Like_Condition_Sql">
	  <if test="inspectCodeLike != null">
	    <bind name="pattern" value="'%' + _parameter.inspectCodeLike + '%'" />
	       AND A.INSPECT_CODE LIKE #{pattern}
	  </if>
	  <if test="fromBillIdlist != null">
	        AND (A.FROM_BILL_ID IN
			<foreach item="item" index="index" collection="fromBillIdlist" open="("
				separator="," close=")">
				#{item}
			</foreach>) 
	  </if>
	</sql>
	
	<sql id="Exp_condition_Sql">
	</sql>
		
	<sql id="Order_By_Sql">
 		order by id desc
	</sql>		
	
	<insert id="insertBillInspectBatch" flushCache="true" parameterType="java.util.List">
		INSERT INTO BILL_INSPECT (
		TENANT_ID,
		INSPECT_CODE,
		SUPPLIER_ID,
		FACTORY_ID,
		AUDIT_STATUS_DICT_ID,
		FROM_BILL_TYPE_DICT_ID,
		FROM_BILL_ID,
		FROM_BILL_CODE,
		MATERIAL_ID,
		INSPECT_MODE_DICT_ID,
		INSPECT_STATUS_DICT_ID,
		INSPECT_RESULT_DICT_ID,
		DEAL_RESULT_DICT_ID,
		QUANTITY,
		QUANTITY_SAMPLE,
		QUANTITY_SAMPLE_FAIL,
		REMARK,
		ID,
		VERSION,
		DEL_FLAG,
		CREATE_USER_ID,
		MODIFY_USER_ID,
		CREATE_TIME,
		MODIFY_TIME
		)
		VALUES
		<foreach collection="list" item="item" separator=",">
		(	
		#{item.tenantId},
		#{item.inspectCode},
		#{item.supplierId},
		#{item.factoryId},
		#{item.auditStatusDictId},
		#{item.fromBillTypeDictId},
		#{item.fromBillId},
		#{item.fromBillCode},
		#{item.materialId},
		#{item.inspectModeDictId},
		#{item.inspectStatusDictId},
		#{item.inspectResultDictId},
		#{item.dealResultDictId},
		#{item.quantity},
		#{item.quantitySample},
		#{item.quantitySampleFail},
		#{item.remark},
		#{item.id},1,0,#{item.createUserId},#{item.modifyUserId} 
		,now()
		,now()
		)
		</foreach>
	</insert>
