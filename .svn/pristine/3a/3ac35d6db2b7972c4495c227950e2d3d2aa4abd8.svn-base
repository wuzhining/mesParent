<mapper namespace="com.techsoft.mapper.config.ConfigCodeRuleItemMapper">
	<resultMap id="ConfigCodeRuleItem" type="com.techsoft.entity.common.ConfigCodeRuleItem" 
			   extends="com.techsoft.common.mapper.BaseEntityMapper.BaseEntityMap" > 
