<script type="text/javascript">
layui.config({
    base: '/static/plugins/layui/base/',
    v: new Date().getTime() 
}).use([ 'form','table','laydate','layuiconfig','layuiformajax','layuitable'], function(){
	var form = layui.form, 
	$ = layui.jquery, table = layui.table,  laydate = layui.laydate,
	layer = layui.layer,  layuitable = layui.layuitable,layuiconfig = layui.layuiconfig;
	var layuiformajax = layui.layuiformajax;
	
	var curTable = layuitable.render({
    	elem: '#listTable'
    	,id:'listTable'
    	,url:'/warehouse/warehouseLocation/list/json'
    	,cols: [[
    	    {field:'id', width:120, title: 'ID',align:'center'}
    	    ,{field:'factoryName', width:150, title: '所属工厂', align:'center',templet: '#factoryName2'}
    	    ,{field:'warehouseName', width:150, title: '仓库', align:'center',templet: '#warehouseName2'}
    	    ,{field:'productTypeDictId',  title: '产品类型',align:'center', minWidth: 120,templet:'#productTypeDictIdTpl'}
    	    ,{field:'locationTypeDictId',  title: '库位类型',align:'center', minWidth: 120,templet:'#locationTypeDictIdTpl'}
    	    ,{field:'locationCode',  title: '货位编码', align:'center',minWidth: 100}
    	    ,{field:'locationName',  title: '货位名称', align:'center',minWidth: 100}    	 
    	    ,{field:'areaName', width:150, title: '仓库区域名称', align:'center',templet: '#areaNameTpl'}
    	    ,{field:'rackName', width:150, title: '货架名称', align:'center',templet: '#rackName2'}
            ,{field:'serialLine',  title: '第几行数', align:'center',minWidth: 100}
    	    ,{field:'serialColumn',  title: '第几列数', align:'center',minWidth: 100}   
            ,{field:'coordinateX',  title: '坐标X', align:'center',minWidth: 100}
    	    ,{field:'coordinateY',  title: '坐标Y', align:'center',minWidth: 100} 
    	    ,{field:'coordinateZ',  title: '坐标Z', align:'center',minWidth: 100}
            ,{field:'limitVolume',  title: '体积限制', align:'center',minWidth: 100}
    	    ,{field:'limitWeight',  title: '重量限制', align:'center',minWidth: 100} 
    	    ,{field:'limitQuantity',  title: '数量限制', align:'center',minWidth: 100} 
    	    ,{field:'limitQuantityBox',  title: '箱数限制', align:'center',minWidth: 100} 
    	    ,{field:'limitQuantityPallet',  title: '托盘限制', align:'center',minWidth: 100} 
            ,{field:'sizeLength',  title: '尺寸-长', align:'center',minWidth: 100}
    	    ,{field:'sizeHeight',  title: '尺寸-高', align:'center',minWidth: 100} 
    	    ,{field:'sizeWidth',  title: '尺寸-宽', align:'center',minWidth: 100} 
    	    ,{field:'isMixProduct',  title: '是否允许混放产品', align:'center',minWidth: 100,templet:'#isMixProductTpl'}
    	    ,{field:'isMixBatch',  title: '是否允许混放批次', align:'center',minWidth: 100,templet:'#isMixBatchTpl'}
    	    ,{field:'isValid',  title: '是否有效', align:'center',minWidth: 120,templet:'#isValidTpl'}
    	    ,{field:'remark',  title: '备注', align:'center',minWidth: 100}
    	    ,{field:'createTime',  title: '创建时间', align:'center',minWidth: 100}
      		,{field:'', width:'5%', title: '操作', align:'center', toolbar: '#barDemo', fixed: 'right'}
    	]]
  	});
	
	form.on('submit(search)', function (data) {
		layuitable.reload(data.field);
        return false;
  	});
 
	
	table.on('tool(tablefilter)', function(obj) {
		var data = obj.data;
		var factoryName = data.structFactory.factoryName;
		var warehouseName = data.structWarehouse.warehouseName;
		var rackName = data.warehouseRack.rackName;
		if (obj.event === 'edit') { 
			
			layer_window_show('编辑仓库库位','/warehouse/warehouseLocation/edit?id='+ data.id +'&factoryName='+factoryName+'&warehouseName='+warehouseName+'&rackName='+rackName+'&edited='+true); 
			
		}
	});
	
	$('#factorySearch').on('click',function(e){
   		openWinSelect('/struct/structFactory/structFactoryOpen',"factoryId","factoryName");
   		e.preventDefault();//默认from表单提交时不刷新
   	})
	$('#warehouseSearch').on('click',function(e){
   		openWinSelect('/struct/structWarehouse/structWarehouseOpen',"warehouseId","warehouseName");
   		e.preventDefault();//默认from表单提交时不刷新
   	})
	$('#rackSearch').on('click',function(e){
   		openWinSelect('/warehouse/warehouseRack/warehouseRackOpen',"rackId","rackName");
   		e.preventDefault();//默认from表单提交时不刷新
   	})
   	//清空
	$("#factoryName").blur(function(){
		if($('#factoryName').val() == ""){
			$('#factoryId').val("");
		}
	});
	$("#warehouseName").blur(function(){
		if($('#warehouseName').val() == ""){
			$('#warehouseId').val("");
		}
	});
	$("#rackName").blur(function(){
		if($('#rackName').val() == ""){
			$('#rackId').val("");
		}
	});
// 	laydate.render({
// 		elem: '#startTime' //指定元素
// 		,type: 'date'
// 		,format: 'yyyy-MM-dd' //可任意组合
// 	});
	
// 	laydate.render({
// 		elem: '#endTime' //指定元素
// 		,type: 'date'
// 		,format: 'yyyy-MM-dd' //可任意组合
// 	});
});	
</script>
 
