	{{#  if(d.configAreaP){ }}
		{{ d.configAreaP.areaName 

 }}
	{{#  } }}
</script> 

<script type="text/html" id="areaNameC">
	{{#  if(d.configAreaC){ }}
		{{ d.configAreaC.areaName 

 }}
	{{#  } }}
