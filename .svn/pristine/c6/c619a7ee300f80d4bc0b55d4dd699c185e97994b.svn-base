		SELECT
		<include refid="Base_Column_List" />
		FROM STRUCT_PROCESS_PRODUCT A
		WHERE (A.DEL_FLAG = 0) and (A.ID=#{id})
	</select>
	
	<select id="selectByIds" parameterType="java.util.List" useCache="false"
			resultType="com.techsoft.entity.common.StructProcessProduct">
		SELECT
		<include refid="Base_Column_List" />
		FROM STRUCT_PROCESS_PRODUCT A WHERE (A.DEL_FLAG = 0) and (A.ID IN
		<foreach item="item" index="index" collection="list" open="("
			separator="," close=")">
			#{item}
		</foreach>)
	</select>

