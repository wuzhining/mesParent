package com.techsoft.entity.alarm;

import com.techsoft.entity.common.AlarmProblem;
import com.techsoft.entity.common.AlarmReason;

public class AlarmReasonVo extends AlarmReason {
	private static final long serialVersionUID = -6887742293866517640L;
	private AlarmProblem alarmProblem;

	public AlarmReasonVo() {	
	}
	
	public AlarmReasonVo(AlarmReason value) {
		value.cloneProperties(this);
	}	
	public AlarmProblem getAlarmProblem() {
		return alarmProblem;
	}

	public void setAlarmProblem(AlarmProblem alarmProblem) {
		this.alarmProblem = alarmProblem;
	}
}
