 </script>
  <script>

  	var url = $(window.parent.document).find("iframe").attr("src");
	var arr = url.split('=');
	var ids = arr[1];
	var billStatusDictId=window.parent.billStatusDictId;
     layui.config({
        base: '/static/plugins/layui/base/',
        v: new Date().getTime()
    }).use([ 'form','table','laydate','layuiconfig','layuiformajax','layuitable'], function(){
    	var form = layui.form, 
    	$ = layui.jquery, table = layui.table,  laydate = layui.laydate,
    	layer = layui.layer,  layuitable = layui.layuitable,layuiconfig = layui.layuiconfig;
    	var layuiformajax = layui.layuiformajax;
    	
   	 $('#productSearch').on('click',function(e){
   		openWinSelect('/product/productMain/productMainOpen',"productId","productName");
   		e.preventDefault();//默认from表单提交时不刷新
   	})
   	$('#search').on('click',function(e){
       		openWinSelect('/product/productMaterial/productMaterialOpen',"materialId","materialName");
       		e.preventDefault();//默认from表单提交时不刷新
       	})
    	
    	var curTable = layuitable.render({
        	elem: '#listTable'
        	,id:'listTable'
        	,url:'/bill/billSaleorderItem/list/json?billSaleorderId='+ids
        	,cols: [[
            	     /* {type: 'checkbox', fixed: 'left'}, */
             	    /*  {field:'billSaleorderId',  title: '销售单',align:'center'}, */
             	   {field:'billCode',  title: '销售单编码',align:'center',templet: '#fbillCode'}
             	    ,{field:'productTypeDictId',  title: '物品类型',align:'center',templet: '#productTypeName'}
             	   , {field:'productName',  title: '物品名称',align:'center',templet:'#productName2'}
             	   ,{field:'materialCode',  title: '物品规格编码',align:'center',templet:'#materialCode2'}
             	    ,{field:'materialName',  title: '物品规格名称',align:'center', minWidth: 350,templet:'#materialName2'}
             	    ,{field:'sku', title: '物品规格值', align:'center',templet: '#sku'}
             	    ,{field:'billStatusDictName',  title: '状态',align:'center',templet:'#billStatusDictName'}
             	    ,{field:'quantity',  title: '单据数量',align:'center'}
	           	    ,{field:'quantityFinish',  title: '完成数量',align:'center'}
	           	    ,{field:'remark',  title: '备注', align:'center',minWidth: 200}
	           	 	,{field:'createTime',  title: '创建时间', align:'center'}
	           	 	,{field:'', width:'20%', title: '操作', align:'center', toolbar: '#barDemo', fixed: 'right'}
        	]]
      	});
    	
    	form.on('submit(search)', function (data) {
    		layuitable.reload(data.field);
            return false;
      	});
    	
    	//监听提交
        form.on('submit(save)', function(data){
      	  console.log(data.field);
      	  var url = '/bill/billSaleorder/save';
      	  layuiformajax.postJson(url, data.field, function (result) {
    	    	   layer.alert("保存成功", {icon: 6},function () {
    	                // 获得frame索引
    	                var index = parent.layer.getFrameIndex(window.name);
    				  	parent.location.reload(); 
    	                parent.layer.close(index);
    	            });
    		  });
    		  
          return false;
        });
        
    	
    	table.on('tool(tablefilter)', function(obj) {
    		var data = obj.data;
    		if (obj.event === 'edit') { 
    			 var productName=data.productMain.productName;
    	    	 var materialName=data.productMaterial.materialName;
    			layer_window_show('编辑销售单明细','/bill/billSaleorderItem/edit?id=' + data.id+"&&materialName="+materialName); 
    		}else{
    	       	var url = '/bill/billSaleorderItem/del?id='+ data.id;
    	       	layuiformajax.postJson(url, '', function (result) {
    	       		layer.msg('删除成功!', {icon: 1, time: 1000},function () {
	     	            // 获得frame索引
     	                var index = parent.layer.getFrameIndex(window.name);
     				  	location.reload(); 
     	                layer.close(index);
  	     	        });
   	     		});
    		}
    	});
    	laydate.render({
    		elem: '#createTimeBegin' //指定元素
    		,type: 'date'
    		,format: 'yyyy-MM-dd HH:mm:ss' //可任意组合
    	});
    	laydate.render({
    		elem: '#createTimeEnd' //指定元素
    		,type: 'date'
    		,format: 'yyyy-MM-dd HH:mm:ss' //可任意组合
    	});
    });
     
     function insertSaleorderItem(){
    	 layer_window_show('添加销售单明细','/bill/billSaleorderItem/edit?idd='+ids);
     }
     
    </script>
    <script></script>
  </body>

</html>