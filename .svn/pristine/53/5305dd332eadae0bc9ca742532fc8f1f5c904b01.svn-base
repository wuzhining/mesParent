<!DOCTYPE html>
<html>
  
  <head> 
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" /> 
	<#include "../common-header.html"/>
  </head>
   <script type="text/html" id="isValidTpl">
    {{#  if(d.isValid == '1'){ }}
		是
	{{#  }else { }}
		否
	{{#  } }}
  </script> 
  <script type="text/html" id="factoryNameTpl">
	{{#  if(d.structFactory){}}
		{{ d.structFactory.factoryName 
 	}}
	{{# } }}
   </script>
   <script type="text/html" id="floorNameTpl">
	{{#  if(d.structFloor){}}
		{{ d.structFloor.floorName 
 	}}
	{{# } }}
   </script>
   <script type="text/html" id="warehouseTypeDict">
	{{#  if(d.warehouseTypeDic){}}
		{{ d.warehouseTypeDic.dictName 
 	}}
	{{# } }}
   </script>
   <script type="text/html" id="productTypeDict">
	{{#  if(d.productTypeDic){}}
		{{ d.productTypeDic.dictName 
 	}}
	{{# } }}
   </script>
  
  <body>
    <div class="x-nav">
      <span class="layui-breadcrumb"> 
        <a href="">工厂构造</a> <a><cite>仓库</cite></a>
      </span>
       <!--
       <a class="layui-btn layui-btn-small" style="line-height:1.6em;margin-top:3px;float:right" href="javascript:location.replace(location.href);" title="刷新">
       <i class="layui-icon" style="line-height:30px">ဂ</i></a>
       -->
    </div>
    <div class="x-body">
      <div class="layui-row">
        <form class="layui-form layui-col-md12 x-so">
          <input type="text" name="warehouseName"  placeholder="请输入仓库名称" autocomplete="off" class="layui-input">
          <input type="text" name="warehouseCode"  placeholder="请输入仓库编码" autocomplete="off" class="layui-input">
       <select name="warehouseTypeDictId">    
                <option value="">请选择仓库类型</option> 
                 <#if warehouseDicList??>
                     <#list warehouseDicList as item>
			            <option value="${item.id?c}" >${item.dictName}</option>
			         </#list>
			         </#if> 
          </select> 
           <select name="productTypeDictId">    
                <option value="">请选择产品类型</option> 
                 <#if producDictList??>
                     <#list producDictList as item>
			            <option value="${item.id?c}" >${item.dictName}</option>
			         </#list>
			         </#if> 
          </select> 
           <input type="hidden" name="factoryId" id="factoryId" placeholder="请选择工厂" autocomplete="off" class="layui-input">
       	   <input type="text" name="factoryName" id="factoryName" placeholder="请选择工厂" autocomplete="off" class="layui-input">
           <button class="layui-btn layui-btn-sm  " id="factorySearch" style="background-color: #009688;color:#FFFFFF" >
			<i class="layui-icon layui-icon-more" ></i>
		   </button>
           <input type="hidden" name="floorId" id="floorId" placeholder="请选择楼层" autocomplete="off" class="layui-input">
       	   <input type="text" name="floorName" id="floorName" placeholder="请选择楼层" autocomplete="off" class="layui-input">
           <button class="layui-btn layui-btn-sm  " id="floorSearch" style="background-color: #009688;color:#FFFFFF" >
			<i class="layui-icon layui-icon-more" ></i>
		   </button>
          <button type="reset" class="layui-btn layui-btn-sm  " id="reset" style="background-color: #009688;color:#FFFFFF">重置</button>   
          <button class="layui-btn layui-btn-sm"  lay-submit lay-filter="search"><i class="layui-icon">&#xe615;</i></button>
        </form>
      </div>
      <xblock>
        <!-- <button class="layui-btn" onclick="delAll()"><i class="layui-icon"></i>删除</button> -->
        <button class="layui-btn layui-btn-sm" onclick="layer_window_show('添加仓库','/struct/structWarehouse/edit')"><i class="layui-icon"></i>添加</button>
        <span class="x-right" style="line-height:40px"></span>
      </xblock>
       <div class="layui-col-md12">
  	 			<div class="layui-row grid-demo">
            		<div class="layui-col-md12" style="text-align: center;">
        					<table class="layui-hide" id="listTable" lay-filter="tablefilter"></table>
        			</div>
        		</div>
        </div>
     
     

    </div>
<script type="text/html" id="barDemo">	
	<a class="layui-btn layui-btn-xs  layui-btn-normal" lay-event="edit">编辑</a>
</script>    
<script type="text/javascript">
layui.config({
    base: '/static/plugins/layui/base/',
    v: new Date().getTime()
}).use([ 'form','table','laydate','layuiconfig','layuiformajax','layuitable'], function(){
	var form = layui.form, 
	$ = layui.jquery, table = layui.table,  laydate = layui.laydate,
	layer = layui.layer,  layuitable = layui.layuitable,layuiconfig = layui.layuiconfig;
	var layuiformajax = layui.layuiformajax;
	
	var curTable = layuitable.render({
    	elem: '#listTable'
    	,id:'listTable'
    	,url:'/struct/structWarehouse/list/json'
    	,cols: [[
    	    {field:'id', width:120, title: 'ID',align:'center'}
    	    ,{field:'factoryName',  title: '所属工厂', align:'center',minWidth: 120,templet:'#factoryNameTpl'}
    	    ,{field:'floorName',  title: '所属楼层', align:'center',minWidth: 120,templet:'#floorNameTpl'}
    	    ,{field:'warehouseName',  title: '仓库名称',align:'center', minWidth: 120}
    	    ,{field:'warehouseCode',  title: '仓库编码',align:'center', minWidth: 120}
    	    ,{field:'warehouseTypeDict',  title: '仓库类型',align:'center', minWidth: 120,templet:'#warehouseTypeDict'}
    	    ,{field:'productTypeDict',  title: '产品类型',align:'center', minWidth: 120,templet:'#productTypeDict'}
    	    ,{field:'isValid',  title: '是否有效', align:'center',minWidth: 120,templet:'#isValidTpl'}
    	    ,{field:'remark',  title: '备注', align:'center',minWidth: 120}
    	    ,{field:'createTime',  title: '创建时间', align:'center',minWidth: 120}
      		,{field:'', width:'20%', title: '操作', align:'center', toolbar: '#barDemo', fixed: 'right'}
    	]]
  	});
	
	form.on('submit(search)', function (data) {
		console.log(data);
		layuitable.reload(data.field);
        return false;
  	});
	
	
	$('#factorySearch').on('click',function(e){
		openWinSelect('/struct/structFactory/structFactoryOpen',"factoryId","factoryName");
   		e.preventDefault();//默认from表单提交时不刷新
   	})
	
     //打开楼层信息列表
    $('#floorSearch').on('click',function(e){
    	openWinSelect('/struct/structFloor/structFloorOpen',"floorId","floorName");
   		e.preventDefault();//默认from表单提交时不刷新
    })
	
	table.on('tool(tablefilter)', function(obj) {
		var data = obj.data;
		if (obj.event === 'edit') { 
			layer_window_show('编辑仓库','/struct/structWarehouse/edit?id=' + data.id); 
		}
	});
	$('#reset').on('click',function(e){
   		$("input").val("");
   		$("select").val("");
   	})
	
});	
</script>
 
  </body>

</html>