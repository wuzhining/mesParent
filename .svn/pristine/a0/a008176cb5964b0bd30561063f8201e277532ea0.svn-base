<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.techsoft.mapper.firm.FirmPostMapper">
	<resultMap id="FirmPost" type="com.techsoft.entity.common.FirmPost" 
			   extends="com.techsoft.common.mapper.BaseEntityMapper.BaseEntityMap" > 
		<result column="TENANT_ID" property="tenantId" />
		<result column="DEPARTMEN_ID" property="departmenId" />
		<result column="POST_NAME" property="postName" />
		<result column="POST_CODE" property="postCode" />
		<result column="PARENT_ID" property="parentId" />
		<result column="IS_VALID" property="isValid" />
		<result column="REMARK" property="remark" />
	</resultMap>

	<sql id="Base_condition_Sql">
		<if test="tenantId != null">
			AND A.TENANT_ID= #{tenantId}
		</if>
		<if test="departmenId != null">
			AND A.DEPARTMEN_ID= #{departmenId}
		</if>
		<if test="postName != null">
			AND A.POST_NAME= #{postName}
		</if>
		<if test="postCode != null">
			AND A.POST_CODE= #{postCode}
		</if>
		<if test="parentId != null">
			AND A.PARENT_ID= #{parentId}
		</if>
		<if test="isValid != null">
			AND A.IS_VALID= #{isValid}
		</if>
		<if test="remark != null">
			AND A.REMARK= #{remark}
		</if>

		<include refid="com.techsoft.common.mapper.BaseEntityMapper.BaseEntityConditionSql" />		
	</sql>
	
	<sql id="Date_Condition_sql">	
	</sql>

	<sql id="Foreign_Key_Condition_Sql">
	
	</sql>
	
	<sql id="Like_Condition_Sql">
	  <if test="postNameLike != null">
	     <bind name="nameLike" value="'%' + _parameter.postNameLike + '%'" />
	       AND A.POST_NAME LIKE #{nameLike}
	  </if>
	  
	  <if test="postCodeLike != null">
	     <bind name="codeLike" value="'%' + _parameter.postCodeLike + '%'" />
	       AND A.POST_CODE LIKE #{codeLike}
	  </if>
	</sql>
	
	<sql id="Exp_condition_Sql">
	</sql>
		
	<sql id="Order_By_Sql">
	 	order by id desc
	</sql>
			
	<sql id="Base_Column_List">    
		A.TENANT_ID,
		A.DEPARTMEN_ID,
		A.POST_NAME,
		A.POST_CODE,
		A.PARENT_ID,
		A.IS_VALID,
		A.REMARK
		<include refid="com.techsoft.common.mapper.BaseEntityMapper.BaseEntityColumnList" />
	</sql>

	<select id="getIDValue" flushCache="true" useCache="false" resultType="java.lang.Long">
		SELECT next value for FIRM_POST_SEQ AS IDValue
	</select>
	
	<select id="getMaxId" flushCache="true" useCache="false" resultType="java.lang.Long">
		SELECT max(id) as maxId from FIRM_POST
	</select>
		
	<select id="selectById" parameterType="java.lang.Long" useCache="true"
			resultType="com.techsoft.entity.common.FirmPost">
		SELECT
		<include refid="Base_Column_List" />
		FROM FIRM_POST A
		WHERE (A.DEL_FLAG = 0) and (A.ID=#{id})
	</select>
	
	<select id="selectByIds" parameterType="java.util.List" useCache="false"
			resultType="com.techsoft.entity.common.FirmPost">
		SELECT
		<include refid="Base_Column_List" />
		FROM FIRM_POST A WHERE (A.DEL_FLAG = 0) and (A.ID IN
		<foreach item="item" index="index" collection="list" open="("
			separator="," close=")">
			#{item}
		</foreach>)
	</select>

	<update id="deleteById" flushCache="true" parameterType="java.lang.Long">
		UPDATE A SET 
		<include refid="com.techsoft.common.mapper.BaseEntityMapper.BaseEntityDelete" />
		FROM FIRM_POST A WHERE (A.ID=#{id}) 
	</update>
	
	<update id="deleteByIds" flushCache="true" parameterType="java.util.List">
		UPDATE A SET 
		<include refid="com.techsoft.common.mapper.BaseEntityMapper.BaseEntityDelete" />
		FROM FIRM_POST A WHERE (A.ID IN
		<foreach item="item" index="index" collection="list" open="("
			separator="," close=")">
			#{item}
		</foreach>)   
	</update>
		
	<update id="deleteByForeignKey" flushCache="true" parameterType="com.techsoft.entity.common.FirmPost">
		UPDATE A SET 
		<include refid="com.techsoft.common.mapper.BaseEntityMapper.BaseEntityDelete" />
		FROM FIRM_POST A WHERE (1 = 1) 
		<include refid="Foreign_Key_Condition_Sql" />
	</update>

	<insert id="insertEntity" flushCache="true" parameterType="com.techsoft.entity.common.FirmPost">
		INSERT INTO FIRM_POST (
		TENANT_ID,
		DEPARTMEN_ID,
		POST_NAME,
		POST_CODE,
		PARENT_ID,
		IS_VALID,
		REMARK
		<include refid="com.techsoft.common.mapper.BaseEntityMapper.BaseEntityInsertColumn" />		
		)
		VALUES(	
		#{tenantId},
		#{departmenId},
		#{postName},
		#{postCode},
		#{parentId},
		#{isValid},
		#{remark}
		<include refid="com.techsoft.common.mapper.BaseEntityMapper.BaseEntityInsertValue" />		
		)
	</insert>

	<update id="updateEntity" flushCache="true" parameterType="com.techsoft.entity.common.FirmPost">
		UPDATE FIRM_POST SET
		TENANT_ID=#{tenantId},
		DEPARTMEN_ID=#{departmenId},
		POST_NAME=#{postName},
		POST_CODE=#{postCode},
		PARENT_ID=#{parentId},
		IS_VALID=#{isValid},
		REMARK=#{remark},
		<include refid="com.techsoft.common.mapper.BaseEntityMapper.BaseEntityUpdate" />
		WHERE ID=#{id} AND VERSION=#{version}  			
	</update>
	
	<update id="updatePartEntity" flushCache="true" parameterType="com.techsoft.entity.common.FirmPost">
		UPDATE FIRM_POST SET
		<if test="tenantId != null" >
			TENANT_ID=#{tenantId},
		</if>
		<if test="departmenId != null" >
			DEPARTMEN_ID=#{departmenId},
		</if>
		<if test="postName != null" >
			POST_NAME=#{postName},
		</if>
		<if test="postCode != null" >
			POST_CODE=#{postCode},
		</if>
		<if test="parentId != null" >
			PARENT_ID=#{parentId},
		</if>
		<if test="isValid != null" >
			IS_VALID=#{isValid},
		</if>
		<if test="remark != null" >
			REMARK=#{remark},
		</if>
		<include refid="com.techsoft.common.mapper.BaseEntityMapper.BaseEntityUpdate" />
		WHERE ID=#{id} AND VERSION=#{version}  			
	</update>	
	
	<select id="selectListByEntityParamVo" parameterType="com.techsoft.entity.firm.FirmPostParamVo"
		      resultType="com.techsoft.entity.common.FirmPost" useCache="false">
		SELECT
		<include refid="Base_Column_List" />
		FROM FIRM_POST A
		WHERE (A.DEL_FLAG = 0)
		<if test="parent != null">
			AND A.PARENT_ID= #{parent}
		</if>
		<include refid="Base_condition_Sql" />
		<include refid="Date_Condition_sql" />
		<include refid="Foreign_Key_Condition_Sql" />
		<include refid="Like_Condition_Sql" />
		<include refid="Exp_condition_Sql" />
		<include refid="Order_By_Sql" />
	</select>
</mapper>