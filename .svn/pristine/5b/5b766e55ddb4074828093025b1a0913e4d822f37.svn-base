		SELECT
		<include refid="Base_Column_List" />
		FROM QUALITY_INSPECT_ITEM A WHERE (A.DEL_FLAG = 0) and (A.ID IN
		<foreach item="item" index="index" collection="list" open="("
			separator="," close=")">
			#{item}
		</foreach>)
