	<update id="deleteByForeignKey" flushCache="true" parameterType="com.techsoft.entity.common.AdminRoleUser">
		UPDATE  A SET 
		<include refid="com.techsoft.common.mapper.BaseEntityMapper.BaseEntityDelete" />
		FROM ADMIN_ROLE_USER A
		WHERE (1 = 1) 
