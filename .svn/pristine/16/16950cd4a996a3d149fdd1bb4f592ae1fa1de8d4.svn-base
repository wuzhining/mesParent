<script type="text/javascript">
layui.config({
    base: '/static/plugins/layui/base/',
    v: new Date().getTime()
}).use([ 'form','table','laydate','layuiconfig','layuiformajax','layuitable'], function(){
	var form = layui.form, 
	$ = layui.jquery, table = layui.table,  laydate = layui.laydate,
	layer = layui.layer,  layuitable = layui.layuitable,layuiconfig = layui.layuiconfig;
	var layuiformajax = layui.layuiformajax;
	
	var curTable = layuitable.render({
    	elem: '#listTable'
    	,id:'listTable'
    	,height: 350
    	,url:'/bill/billInventory/list/json'
    	,limits:[5,10,15,20]
    	,cols: [[
    	     {field:'billCode', width:120, title: '单据编码',align:'center'}
    	    ,{field:'auditStatusName',  title: '审核状态',align:'center', minWidth: 120,templet:'#auditStatusName'} 
    	    ,{field:'billStatusDic',  title: '单据状态',align:'center', minWidth: 120,templet:'#billStatus'} 
    	    ,{field:'settleStatusName',  title: '结算状态',align:'center', minWidth: 120,templet:'#settleStatusName'} 
    	    ,{field:'Warehouse',  title: '仓库',align:'center', minWidth: 120,templet:'#Warehouse'}
    	    ,{field:'inventoryModeName',  title: '盘点方式',align:'center', minWidth: 120,templet:'#inventoryModeName'}
    	    ,{field:'timeStart',  title: '计划开始时间', align:'center',minWidth: 120}
    	    ,{field:'timeEnd',  title: '计划结束时间', align:'center',minWidth: 120}
    	    ,{field:'createTime',  title: '创建时间', align:'center',minWidth: 120}
    	    ,{field:'', width:'20%', title: '操作', align:'center', templet:'#barDemo', fixed: 'right'}
    	]]
  	});
	
	var renderTable = function (billId) {
		layuitable.render({
	    	elem: '#listTable_details'
	    	,id:'listTable_details'
	    	,url:'/bill/billInventoryItem/list/json?billId='+billId
	    	,limits:[5,10,15,20]
	    	,cols: [[
                 {field:'product', minWidth:160, title: '产品名称',align:'center',templet:'#product'} 
	    	    ,{field:'materialName', minWidth:200, title: '物料名称',align:'center',templet:'#materialName'}
	    	    ,{field:'sku', title: '物料规格值', align:'center',minWidth: 220,templet: '#sku'}
	    	    ,{field:'quantity',  title: '盘点数量',align:'center', minWidth: 80}
	    	    ,{field:'quantityStock',  title: '系统库存数量',align:'center', minWidth: 80}
	    	]]
	  	});
	}
	
	//搜索
	form.on('submit(search)', function (data) {
		var billCode=data.field.billCode!=null?data.field.billCode:"";
		table.reload('listTable',{
			url:'/bill/billInventory/list/json'
			,where:{
				"billCode":billCode
			}
		});
    	return false;
		});
	
	table.on('tool(tablefilter)', function(obj) {
		var data = obj.data;
		if (obj.event === 'edit') { 
			if(data.billStatusDictId =='10488'){
				layer_window_show('编辑单据','/bill/billInventory/edit?id=' + data.id); 
			}else{
				var str='';
				if(data.billStatusDictId==10489){
					str='单据处理中,不可修改！';
				}else if(data.billStatusDictId==10490){
					str='单据已完结,请勿修改！';
				}
				layer.alert("【"+data.billCode+"】"+str);  
			}
		}if(obj.event === 'checkAndAdd'){
				var url = '/bill/billInventoryItem/check?id='+data.id;
		       	  layuiformajax.postJson(url, '', function (result) {
		       		parent.layer.msg('已生成全仓库盘点明细!', {icon: 1, time: 1000},function () {
		     				  	location.reload(); 
		     	            });
		       		 
		     		  });
			 
		}if (obj.event === 'addItem'){
			var url='/bill/billInventoryItem/list?billId='+data.id+'&billStatusDictId='+data.billStatusDictId+'&warehouseId='+data.warehouseId;
			layer_window_show('选择仓库物料盘点',url);
		}if (obj.event === 'checkSuccess'){
			var url = '/bill/billInventory/checkSuccess?id='+data.id;
	       	  layuiformajax.postJson(url, '', function (result) {
	       		parent.layer.msg('审核成功!', {icon: 1, time: 1000},function () {
	     				  	location.reload(); 
	     	            });
	       		 
	     		  });
		}if (obj.event === 'checkFault'){
			var url = '/bill/billInventory/checkFault?id='+data.id;
	       	  layuiformajax.postJson(url, '', function (result) {
	       		parent.layer.msg('审核成功!', {icon: 1, time: 1000},function () {
	     				  	location.reload(); 
	     	            });
	       		 
	     		  });
		}
	});
	
	//初始化
	renderTable(0);
    //监听行单击事件（单击事件为：rowDouble） 
    table.on('row(tablefilter)', function(obj){
    	var data = obj.data;
    	$("#bill").html(data.billCode); 
      	//根据id加载
    	renderTable(data.id);
      	//标注选中样式
      	obj.tr.addClass('layui-table-click').siblings().removeClass('layui-table-click');
    });
	
	
	
	laydate.render({
		elem: '#startTime' //指定元素
		,type: 'date'
		,format: 'yyyy-MM-dd' //可任意组合
	});
	
	laydate.render({
		elem: '#endTime' //指定元素
		,type: 'date'
		,format: 'yyyy-MM-dd' //可任意组合
	});
});	
</script>
 
  </body>

</html>